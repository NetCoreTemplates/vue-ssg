{
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --open",
    "dtos": "cd src && x ts",
    "build": "cross-env NODE_ENV=production vite-ssg build",
    "build:local": "vue-tsc --noEmit && vite-ssg build --mode development",
    "publish": "npm run build && dotnet publish ../api/MyApp/MyApp.csproj -c Release",
    "serve": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "postinstall": "cd ../api/MyApp && dotnet run --AppTasks=migrate",
    "migrate": "cd ../api/MyApp && dotnet run MyApp.csproj --AppTasks=migrate",
    "revert:last": "cd ../api/MyApp && dotnet run --AppTasks=migrate.revert:last",
    "revert:all": "cd ../api/MyApp && dotnet run --AppTasks=migrate.revert:all"
  },
  "dependencies": {
    "@servicestack/client": "^2.0.0",
    "@servicestack/vue": "^3.0.13",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@vueuse/core": "^9.11.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.29",
    "swrv": "^1.0.3",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.14",
    "@iconify-json/emojione": "^1.1.5",
    "@iconify-json/et": "^1.1.3",
    "@iconify-json/fluent": "^1.1.17",
    "@iconify-json/ic": "^1.1.12",
    "@iconify-json/line-md": "^1.1.22",
    "@iconify-json/logos": "^1.1.22",
    "@iconify-json/mdi": "^1.1.44",
    "@iconify-json/noto": "^1.1.10",
    "@iconify-json/octicon": "^1.1.28",
    "@iconify-json/simple-icons": "^1.1.41",
    "@iconify-json/simple-line-icons": "^1.1.3",
    "@types/node": "^18.11.18",
    "@types/nprogress": "^0.2.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/server-renderer": "^3.2.45",
    "@vueuse/head": "^1.0.23",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "front-matter": "^4.0.2",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-icons": "^0.15.1",
    "unplugin-vue-components": "^0.22.12",
    "vite": "^3.2.5",
    "vite-plugin-inspect": "^0.7.14",
    "vite-plugin-pages": "^0.28.0",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-plugin-vue-markdown": "^0.22.2",
    "vite-ssg": "^0.22.1",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.24"
  }
}
